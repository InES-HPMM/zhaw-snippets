{
  "name": "zhaw-snippets",
  "displayName": "ZHAW Snippets",
  "description": "A collection of ZHAW related snippets",
  "icon": "imgs/zhaw.png",
  "version": "1.0.0",
  "publisher": "InES-HPMM",
  "engines": {
    "vscode": "^1.64.0"
  },
 "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InES-HPMM/zhaw-snippets"
  },
  "scripts": {
    "build": "vsce package -o zhaw-snippets-$npm_package_version.vsix",
    "install": "npm run build && code --install-extension zhaw-snippets-$npm_package_version.vsix"
  },
  "keywords": [
    "snippets",
    "zhaw"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "vhdl",
        "path": "./snippets/vhdl.json"
      },
      {
        "language": ["c", "cpp"],
        "path": "./snippets/cpp.json"
      }
    ]
  }
}
